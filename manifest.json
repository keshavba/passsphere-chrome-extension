{
  "manifest_version": 1,
  "name": "PassSphere Extension",
  "version": "1.0",
  "description": "PassSphere chrome extension",
  "content_scripts": [
    {
      "matches": ["https://example.com/login/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}